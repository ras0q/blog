{
  "nodeModulesDir": "auto",
  "exclude": [
    "node_modules"
  ],
  "imports": {
    "lume/": "https://deno.land/x/lume@v3.0.10/",
    "lume/jsx-runtime": "https://deno.land/x/ssx@v0.1.12/jsx-runtime.ts",
    "rss-parser": "npm:rss-parser@3.13.0",
    "unist-util-visit": "npm:unist-util-visit@5.0.0",
    "unocss": "npm:unocss@66.5.2"
  },
  "tasks": {
    "lume": "echo \"import 'lume/cli.ts'\" | deno run -A -",
    "build": "deno task lume",
    "serve": "deno task lume -s"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "lume",
    "types": [
      "lume/types.ts"
    ]
  },
  "unstable": [
    "temporal",
    "fmt-component"
  ],
  "lint": {
    "plugins": [
      "https://deno.land/x/lume@v3.0.10/lint.ts"
    ],
    "rules": {
      "exclude": [
        "no-import-prefix"
      ]
    }
  }
}
